{
  "name": "chocofamilyme/laravel-voicecall",
  "description": "package to make voice calls",
  "keywords": [
    "laravel",
    "voicecall",
    "zvonobot"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "IT rahmetapp.kz",
      "homepage": "https://rahmetapp.kz"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "^4.0",
    "sempro/phpunit-pretty-print": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chocofamilyme\\LaravelVoiceCall\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chocofamilyme\\LaravelVoiceCall\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chocofamilyme\\LaravelVoiceCall\\VoiceCallServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "check-style": "./vendor/bin/phpcs",
    "fix-style": "./vendor/bin/phpcbf"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
